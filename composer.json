{
    "name": "codicastudio/newsletter",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/newsletter",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^8.0",
        "drewm/mailchimp-api": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Newsletter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Newsletter\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Newsletter\\NewsletterServiceProvider"
            ],
            "aliases": {
                "Newsletter": "Spatie\\Newsletter\\NewsletterFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
